$ ->
  if $('#case-analysis-content').size() > 0
    summaryEditor = new wysihtml5.Editor 'case-analysis-content',
      toolbar: 'case-analysis-content-editor-toolbar',
      parserRules: wysihtml5ParserRules,
      stylesheets: ['<%= asset_path 'wysihtml5_editor.css'%>'],
      useLineBreaks: false,
      autoLink: true
