<div class='page-header clearfix'>
  <h1>习题记录</h1>
  <ul>
    <li>
      <%= link_to '导出', admin_student_unit_exams_path(@student, :format => :csv), :class => 'btn btn-default' %>
    </li>
  </ul>
</div>
<table class='table table-striped table-hover'>
  <thead>
    <tr>
      <th>专题</th>
      <th>章节</th>
      <th>单元</th>
      <th>用时</th>
      <th>得分</th>
      <th>考试时间</th>
    </tr>
  </thead>
  <% @unit_exams.each do |unit_exam| %>
    <tr>
      <td><%= unit_exam.topic_name %></td>
      <td><%= unit_exam.chapter_name %></td>
      <td><%= unit_exam.name %></td>
      <td><%= unit_exam.used_minutes %>分钟</td>
      <td><%= unit_exam.score %></td>
      <td><%= l unit_exam.created_at, :format => :long %></td>
    </tr>
  <% end%>
</table>
